{{define "newScheduleGroup"}}
<!DOCTYPE html>
<html lang="ru">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<meta name="theme-color" content="#ff0b0b" />
		<link rel="icon" href="/static/img/favicon.ico" type="image/x-icon" />

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
		<script src="https://telegram.org/js/telegram-web-app.js"></script>
		<script src="/static/js/new_schedule_group.js"></script>

		<link rel="stylesheet" href="/static/css/new_schedule_group.css" />

		<title>Редактирование расписания</title>
		<meta name="description" content="" />
	</head>
	<body>

		<section class="page-contain_input" >
			<a class="data-card_schedule">
				<h2 id="GroupName">{{.GroupName}}</h2>
			</a>
		</section>
		<section class="page-contain_input" id="page-contain_schedule">

			<a class="data-card_schedule" id="schedule">
			<div id="form_schedule">
				<button class="button-28_input" role="button" id="first_button_1" onclick="editShedule()">Собрать расписание.</button>
				<h2>Четная неделя</h2>
				<table>
					<tbody>
						<tr>
							<td><h>Понедельник</h></td>
						</tr>
							{{range $index, $element := .EvenMonday}}
								<tr>
								<td><input class="input_class" type="text" onkeydown="disableEnterKey(event)" 
								placeholder="Предмет"  value="{{$element}}" id="even_monday_{{$index}}" /></td>
								</tr>
							{{end}}
						<tr>
							<td>
								<button class="button_plus" type="button" id="even_monday_plus_button">+</button>
								<button class="button_minus" type="button" id="even_monday_minus_button">-</button>
							</td>
						</tr>

						<tr>
							<td><h>Вторник</h></td>
						</tr>
						
							{{range $index, $element := .EvenTuesday}}
