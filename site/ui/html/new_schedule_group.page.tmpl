								<td><input class="input_class" type="text" onkeydown="disableEnterKey(event)" 
								placeholder="Предмет"  value="{{$element}}" id="even_wednesday_{{$index}}" /></td>
							</tr>{{end}}
						
						<tr>
							<td>
								<button class="button_plus" type="button" id="even_wednesday_plus_button">+</button>
								<button class="button_minus" type="button" id="even_wednesday_minus_button">-</button>
							</td>
						</tr>

						<tr>
							<td><h>Четверг</h></td>
						</tr>
						
